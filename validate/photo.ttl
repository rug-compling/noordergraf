
:PhotoShape a sh:NodeShape ;
  sh:targetClass :Photo ;
  sh:class :Photo ;
  sh:closed true ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1;
  ] ;
  sh:property [
    sh:path :file ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :geo ;
    sh:class :GeoPoint ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path :nd ;
    sh:maxCount 1 ;
    sh:datatype ll: ;
    sh:pattern "^[-+][0-9]{2}\\.[0-9]{4}[-+][0-9]{3}\\.[0-9]{4}$"
  ] ;
  sh:property [
    sh:path :dcDate ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] ;
  sh:property [
    sh:path :dcLicense ;
    sh:maxCount 1 ;
    sh:or (
      [ sh:nodeKind sh:IRI ]
      [ sh:datatype xsd:string ]
    )
  ] ;
  sh:property [
    sh:path :dcCreator ;
    sh:or (
      [ sh:nodeKind sh:IRI ]
      [ sh:datatype xsd:string ]
    )
  ] .
