
:SymbolClassShape a sh:NodeShape ;
  sh:closed true ;
  sh:targetClass :SymbolClass ;
  sh:class :SymbolClass ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path rdfs:comment ;
    sh:datatype rdf:langString ;
    sh:uniqueLang true ;
    # Let op: validator kijkt alleen naar eerste twee letters
    sh:languageIn ("eng" "nld") ;
    sh:minCount 2 ;
    sh:maxCount 2 
  ] ;
  sh:property [
    sh:path rdfs:seeAlso ;
    sh:nodeKind sh:IRI
  ] ;
  sh:property [
    sh:path :image ;
    sh:class :Photo ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path dc:modified ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] .
