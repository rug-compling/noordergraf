# TODO: DELETE ??


:pictogramPartShape a sh:NodeShape ;
  sh:closed true ;
  sh:targetClass :PictogramPart ;
  sh:class :PictogramPart ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1;
  ] ;
  sh:property [
    sh:path :dcTitle ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] ;
  sh:property [
    sh:path :dcCreator ;
    sh:or (
      [ sh:nodeKind sh:IRI ]
      [ sh:datatype xsd:string ]
    )
  ] ;
  sh:property [
    sh:path :dcSource ;
    sh:nodeKind sh:IRI
  ] .
