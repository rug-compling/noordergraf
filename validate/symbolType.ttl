
:SymbolTypeShape a sh:NodeShape ;
  sh:closed true ;
  sh:targetClass :SymbolType ;
  sh:class :SymbolType ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :symbolDescription ;
    sh:datatype rdf:langString ;
    sh:uniqueLang true ;
    # Let op: validator kijkt alleen naar eerste twee letters
    sh:languageIn ("eng" "nld") ;
    sh:minCount 2 ;
    sh:maxCount 2 
  ] ;
  sh:property [
    sh:path :seeAlso ;
    sh:nodeKind sh:IRI
  ] ;
  sh:property [
    sh:path :sameAs ;
    sh:nodeKind sh:IRI
  ] ;
  sh:property [
    sh:path :pictogram ;
    sh:class :Pictogram ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :dcModified ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] .
