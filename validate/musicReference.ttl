:MusicReferenceShape a sh:NodeShape ;
  sh:targetClass :MusicReference ;
  sh:class :MusicReference ;
  sh:closed true ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :artist ;
    sh:datatype xsd:string
  ] ;
  sh:property [
    sh:path :title ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    ) ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :albumTitle ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    ) ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :albumTrack ;
    sh:datatype xsd:string ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :date ;
    sh:or (
      [ sh:datatype xsd:date ]
      [ sh:datatype xsd:gYearMonth ]
      [ sh:datatype xsd:gYear ]
    ) ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path :discogs ;
    sh:nodeKind sh:IRI ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :comment ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] .

