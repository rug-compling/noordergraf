
:PlaceShape a sh:NodeShape ;
  sh:targetClass :Place ;
  sh:class :Place ;
  sh:closed true ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :placename ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :geoCode ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :geoMap ;
    sh:nodeKind sh:IRI ;
    # TODO ??
    # sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :geoFeature ;
    sh:nodeKind sh:IRI ;
    # TODO ??
    # sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :geo ;
    sh:class geo:Point ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :nd ;
    sh:datatype ll: ;
    sh:pattern "^[-+][0-9]{2}\\.[0-9]{4}[-+][0-9]{3}\\.[0-9]{4}$" ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :pluscode ;
    sh:datatype xsd:string ;
    sh:maxCount 1
    # TODO
  ] ;
  sh:property [
    sh:path :osm ;
    sh:nodeKind sh:IRI ;
    sh:maxCount 1
    # TODO: REMOVE ???
  ] ;    
  sh:property [
    sh:path :wikidata ;
    sh:nodeKind sh:IRI ;
    sh:maxCount 1
  ] ;    
  sh:property [
    sh:path :wikipedia ;
    sh:nodeKind sh:IRI ;
    sh:maxCount 1
    # TODO: REMOVE
  ] ;    
  sh:property [
    sh:path dc:modified ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] .
