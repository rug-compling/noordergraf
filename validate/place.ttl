
:PlaceShape a sh:NodeShape ;
  sh:targetClass :Place ;
  sh:class :Place ;
  sh:closed true ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :placename ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :geo ;
    sh:class geo:Point ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :nd ;
    sh:datatype ll: ;
    sh:pattern "^[-+][0-9]{2}\\.[0-9]{4}[-+][0-9]{3}\\.[0-9]{4}$" ;
    sh:minCount 1 ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path shema:sameAs ;
    sh:nodeKind sh:IRI
  ] ;    
  sh:property [
    sh:path dc:modified ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] .
