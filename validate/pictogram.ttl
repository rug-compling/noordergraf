
:PictogramShape a sh:NodeShape ;
  sh:targetClass :Pictogram ;
  sh:class :Pictogram ;
  sh:closed true ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1;
  ] ;
  sh:property [
    sh:path :file ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path dc:date ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] ;
  sh:property [
    sh:path dc:license ;
    sh:maxCount 1 ;
    sh:or (
      [ sh:nodeKind sh:IRI ]
      [ sh:datatype xsd:string ]
    )
  ] ;
  sh:property [
    sh:path dc:source ;
    sh:maxCount 1 ;
    sh:nodeKind sh:IRI
  ] ;
  sh:property [
    sh:path dc:creator ;
    sh:or (
      [ sh:nodeKind sh:IRI ]
      [ sh:datatype xsd:string ]
    )
  ] ;
  sh:property [
    sh:path dc:title ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] ;
  sh:property [
    # TODO: REMOVE
    sh:path :pictogramParts ;
    sh:class :PictogramPart
  ] .
