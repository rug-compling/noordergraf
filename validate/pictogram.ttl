
:PictogramShape a sh:NodeShape ;
  sh:targetClass :Pictogram ;
  sh:class :Pictogram ;
  sh:closed true ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1;
  ] ;
  sh:property [
    sh:path :file ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :dcLicense ;
    sh:maxCount 1 ;
    sh:or (
      [ sh:nodeKind sh:IRI ]
      [ sh:datatype xsd:string ]
    )
  ] ;
  sh:property [
    sh:path :dcSource ;
    sh:maxCount 1 ;
    sh:nodeKind sh:IRI
  ] ;
  sh:property [
    sh:path :dcCreator ;
    sh:or (
      [ sh:nodeKind sh:IRI ]
      [ sh:datatype xsd:string ]
    )
  ] ;
  sh:property [
    sh:path :dcTitle ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] ;
  sh:property [
    # TODO: REMOVE
    sh:path :pictogramParts ;
    sh:class :PictogramPart
  ] .
