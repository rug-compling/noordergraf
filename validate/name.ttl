
:NameShape a sh:NodeShape ;
  sh:closed true ;
  sh:targetClass :Name ;
  sh:class :Name ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1;
  ] ;
  sh:property [
    sh:path :fullname ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype xsd:string
  ] ;
  sh:property [
    sh:path :fullnameSE ;
    sh:maxCount 1 ;
    sh:datatype xsd:string
  ] ;
  sh:property [
    sh:path :suffix ;
    sh:maxCount 1 ;
    sh:datatype xsd:string
  ] ;
  sh:property [
    sh:path :prefix ;
    sh:maxCount 1 ;
    sh:datatype xsd:string
  ] ;
  sh:property [
    sh:path :text ;
    sh:minCount 1 ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] .

