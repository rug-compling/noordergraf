
:TombShape a sh:NodeShape ;
  sh:targetClass :Tomb ;
  sh:class :Tomb ;
  sh:closed true ;
  sh:nodeKind sh:IRI ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1; 
  ] ;
  sh:property [
    sh:path :text ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] ;
  sh:property [
    sh:path :comment ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] ;
  sh:property [
    sh:path :commemorator ;
    sh:class :Person
  ] ;
  sh:property [
    sh:path :creator ;
    sh:class :Creator ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :picture ;
    sh:class :PictureClass
  ] ;
  sh:property [
    sh:path :image ;
    sh:class :Photo
  ] ;
  sh:property [
    sh:path :site ;
    sh:class :Site ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :subject ;
    sh:class :Person ;
    sh:nodeKind sh:IRI
  ] ;
  sh:property [
    sh:path :quote ;
    sh:class :Quote
  ] ;
  sh:property [
    sh:path :symbol ;
    sh:class :Symbol
  ] ;
  sh:property [
    sh:path :date ;
    sh:maxCount 1 ;
    sh:or (
      [ sh:datatype xsd:date ]
      [ sh:datatype xsd:gYearMonth ]
      [ sh:datatype xsd:gYear ]
    )
  ] ;
  sh:property [
    sh:path dc:modified ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] .
