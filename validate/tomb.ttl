
:TombShape a sh:NodeShape ;
  sh:targetClass :Tomb ;
  sh:class :Tomb ;
  sh:closed true ;
  sh:nodeKind sh:IRI ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1; 
  ] ;
  sh:property [
    sh:path :text ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] ;
  sh:property :MaybeImage ;
  sh:property [
    sh:path :site ;
    sh:class :Site ;
    sh:nodeKind sh:IRI ;
    sh:maxCount 1
  ] ;
  sh:property :MaybeSubject ;
  sh:property :MaybeQuote ;
  sh:property [
    sh:path :symbol ;
    sh:class symbol:Symbol ;
    sh:property [
      sh:path :comment ;
      sh:or (
        [ sh:datatype xsd:string ]
        [ sh:datatype rdf:langString ]
      )
    ]
  ] ;
  sh:property [
    sh:path :date ;
    sh:datatype :Date ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path dc:modified ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] .

:MaybeQuote sh:path :quote ;
  sh:class :Quote ;
  sh:property [
    sh:path :text ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] ;
  sh:property [
    sh:path :reference ;
    sh:class :Reference ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :target ;
    sh:maxCount 1 ;
    sh:class :Person
  ] .

:MaybeImage sh:path :image ;
  sh:class :Photo ;
  sh:property [
    sh:path :file ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :geo ;
    sh:class geo:Point ;
    sh:maxCount 1 ;
    sh:property [
      sh:path geo:lat ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:decimal
    ] ;
    sh:property [
      sh:path geo:long ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:decimal
    ]
  ] ;
  sh:property [
    sh:path :nd ;
    sh:maxCount 1 ;
    sh:datatype ll:
  ] ;
  sh:property [
    sh:path dc:date ;
    sh:maxCount 1 ;
    sh:datatype xsd:dateTime
  ] ;
  sh:propert [
    sh:path dc:license ;
    sh:maxCount 1 ;
    sh:or (
      [ sh:nodeKind sh:IRI ]
      [ sh:datatype xsd:string ]
    )
  ] .

  
:MaybeSubject sh:path :subject ;
  sh:class :Person ;
  sh:nodeKind sh:IRI ;
  sh:property :HasName .

:HasName sh:path :name ;
  sh:minCount 1 ;
  sh:maxCount 1 ; 
  sh:property [
    sh:class :Name ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:property [
      sh:path :fullname ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:string
    ] ;
    sh:property [
      sh:path :text ;
      sh:or (
        [ sh:datatype xsd:string ]
        [ sh:datatype rdf:langString ]
      )
    ]
  ] .
