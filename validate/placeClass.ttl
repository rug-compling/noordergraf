
:PlaceClassShape a sh:NodeShape ;
  sh:closed true ;
  sh:targetClass :PlaceClass ;
  sh:class :PlaceClass ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1;
  ] ;
  sh:property [
    sh:path :place ;
    sh:class :Place ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :coordinates ;
    sh:class :Coordinates ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :partOf ;
    sh:class :PlaceClass
  ] ;
  sh:property [
    sh:path :text ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] ;
  sh:property [
    sh:path :comment ;
    sh:or (
      [ sh:datatype xsd:string ]
      [ sh:datatype rdf:langString ]
    )
  ] .

