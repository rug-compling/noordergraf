
:OneText 
  sh:path :text ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
  sh:or (
    [ sh:datatype xsd:string ]
    [ sh:datatype rdf:langString ]
  ) .

:MaybeText 
  sh:path :text ;
  sh:maxCount 1 ;
  sh:or (
    [ sh:datatype xsd:string ]
    [ sh:datatype rdf:langString ]
  ) .

:OneOrMoreText 
  sh:path :text ;
  sh:minCount 1 ;
  sh:or (
    [ sh:datatype xsd:string ]
    [ sh:datatype rdf:langString ]
  ) .

:AnyText 
  sh:path :text ;
  sh:or (
    [ sh:datatype xsd:string ]
    [ sh:datatype rdf:langString ]
  ) .

:AnyComment
  sh:path :comment ;
  sh:or (
    [ sh:datatype xsd:string ]
    [ sh:datatype rdf:langString ]
  ) .
