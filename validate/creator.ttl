
:CreatorShape a sh:NodeShape ;
  sh:closed true ;
  sh:targetClass :Creator ;
  sh:class :Creator ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1;
    sh:maxCount 1;
  ] ;
  sh:property [
    sh:path :name ;
    sh:class :Name ;
    sh:maxCount 1
  ] ;
  sh:property [
    sh:path :location ;
    sh:class :LocationClass ;
    sh:maxCount 1
  ] .
