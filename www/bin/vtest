#!/bin/bash

echo Content-type: text/plain
echo

echo Dit is `pwd`/`basename $0`
echo

echo "REAL:      `id -rnu`:`id -rng`"
echo  EFFECTIVE: `id`
echo

PATH=/usr/lib/go-1.15/bin:/opt/agraph/bin:$PATH

cat /etc/os-release 2>&1
echo
uname -a
echo

echo PATH:
echo $PATH | perl -p -e 's!:!\n!g'
echo

echo HOSTNAME:
hostname -f
hostname -I
echo

echo LOCALE:
locale
echo

echo LOCALE -a:
locale -a
echo

echo LDD:
ldd --version 2>&1
echo

for i in \
    agraph \
    go
do
    echo "$i : `which $i 2>&1`"
done
echo

echo AGRAPH:
agraph --version
echo

echo GO:
go version
echo
