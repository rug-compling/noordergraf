#!/bin/bash

echo Content-type: text/plain
echo

echo Dit is `pwd`/`basename $0`
echo

echo "REAL:      `id -rnu`:`id -rng`"
echo  EFFECTIVE: `id`
echo

cat /etc/os-release 2>&1
echo
uname -a
echo

echo PATH:
echo -e ${PATH//:/\\n}
echo

echo HOSTNAME:
hostname -f
hostname -I
echo

echo LOCALE:
locale
echo

echo LOCALE -a:
locale -a
echo

echo LDD:
ldd --version 2>&1
echo

PATH=/opt/agraph/bin:$PATH

for i in \
    agraph \
    go
do
    type -a $i 2>&1
done
echo

echo AGRAPH:
agraph --version 2>&1
echo

echo GO:
go version
echo
