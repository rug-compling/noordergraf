#!/bin/bash

set -e

. secret.sh

agtool load \
       --overwrite \
       --bulk \
       --duplicates delete \
       --automate-nd-datatype-mappings \
       --input trig \
       --optimize \
       --base-uri https://noordergraf.rug.nl/ \
       noordergraf \
       -

echo

python3 mkfti.py
../tools/chkll
../tools/chktags
../tools/mksymidx > ../www/sym/index.body.eng
../tools/mksymidx -n > ../www/sym/index.body.nld

echo OK
echo
